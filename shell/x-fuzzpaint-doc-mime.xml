<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x.fuzzpaint-doc">
    <comment>FZP Fuzzpaint Document</comment>
    <acronym>FZP</acronym>
    <expanded-acronym>FuZzPaint</expanded-acronym>
    <sub-class-of type="application/x-riff"/>
    <glob pattern="*.fzp"/>
    <icon name="application-x-fuzzpaint-doc"/>
    <generic-icon name="image-x-generic"/>
    <magic>
      <match type="string" offset="0" value="RIFF">
        <match type="string" offset="8" value="fzp "/>
      </match>
    </magic>
  </mime-type>
</mime-info>
